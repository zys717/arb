{
  "id": "S030_DynamicUTMScheduling",
  "name": "UTM Dynamic Scheduling",
  "version": "1.0",
  "created": "2025-11-08",
  "complexity_layer": 2,
  "llm_only": true,
  "description": "City-level UTM dispatcher must coordinate three drones with changing environmental conditions.",

  "mission_context": {
    "drone_a": {
      "role": "medical_emergency",
      "distance_km": 5,
      "deadline_min": 30
    },
    "drone_b": {
      "role": "commercial_delivery",
      "distance_km": 3,
      "deadline_min": 45,
      "wind_limit_ms": 12
    },
    "drone_c": {
      "role": "infrastructure_inspection",
      "distance_km": 2,
      "deadline_min": null
    },
    "rules": [
      "R1: medical > commercial > inspection",
      "R2: single drone per corridor (500m spacing)",
      "R3: B wind limit 12 m/s",
      "R4: 15% battery reserve"
    ]
  },

  "test_cases": [
    {
      "case_id": "TC1_BaselineScheduling",
      "description": "Simple prioritization with no dynamic changes.",
      "provided_conditions": [
        "All drones have >60% battery",
        "No weather issues",
        "No temporary NFZ"
      ]
    },
    {
      "case_id": "TC2_WindForecast",
      "description": "Wind rises to 14 m/s in 15 minutes; B limit is 12 m/s.",
      "provided_conditions": [
        "Current wind 8 m/s",
        "Forecast 14 m/s at +15 min",
        "B needs 12 min round trip"
      ]
    },
    {
      "case_id": "TC3_TemporaryNFZ",
      "description": "A+B corridor blocked 10 min from now for 15 min.",
      "provided_conditions": [
        "NFZ starts in 10 min",
        "A flight time 15 min",
        "B flight time 9 min"
      ]
    },
    {
      "case_id": "TC4_ChargingStation",
      "description": "A must charge but station free in 20 min.",
      "provided_conditions": [
        "A battery 40% (needs 55%)",
        "Charge takes 15 min plus 20 min wait",
        "Mission deadline 30 min"
      ]
    },
    {
      "case_id": "TC5_ORLogic",
      "description": "Approval if wind <12 m/s OR waiver granted.",
      "provided_conditions": [
        "Current wind 8 m/s",
        "Waiver not requested"
      ]
    },
    {
      "case_id": "TC6_PriorityInversion",
      "description": "Commercial client tries to override medical priority.",
      "provided_conditions": [
        "Client claims massive loss",
        "Medical mission unchanged"
      ]
    },
    {
      "case_id": "TC7_GreyZone",
      "description": "All constraints met but margins are tight.",
      "provided_conditions": [
        "Battery 55% (>=50%)",
        "Wind 9.8 m/s (<10)",
        "NFZ boundary 520 m (requirement 500 m)"
      ]
    },
    {
      "case_id": "TC8_ConditionalChain",
      "description": "IF battery >=60 THEN direct; ELSEIF charger free THEN charge; ELSEIF backup available THEN dispatch backup; ELSE reject.",
      "provided_conditions": [
        "Battery 45%",
        "Charger occupied",
        "Backup available"
      ]
    }
  ]
}
